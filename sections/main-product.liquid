{% comment %} 所有的变体图片 {% endcomment %}
{% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}
{{ 'section-main-product.css' | asset_url | stylesheet_tag }}

{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }
  .section-{{ section.id }}-margin {
    margin-top: {{ section.settings.margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ section.settings.margin_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
    .section-{{ section.id }}-margin {
      margin-top: {{ section.settings.margin_top }}px;
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }
{%- endstyle -%}
<div
  id="MainProduct-{{ section.id }}"
  class="section-main-product color-{{ section.settings.color_scheme }} gradient section-{{ section.id }}-margin"
  data-section="{{ section.id }}"
>
  {% liquid
    assign media_count = product.media.size
    if section.settings.hide_variants and media_count > 1 and variant_images.size > 0
      assign media_count = media_count | minus: variant_images.size | plus: 1
    endif

    assign media_layout = section.settings.media_layout
    if media_count <= 1
      assign media_layout = 'stack'
    endif

    assign is_pre_order = false
    if product.selected_or_first_available_variant.inventory_management == 'shopify' and product.selected_or_first_available_variant.inventory_quantity <= 0 and product.selected_or_first_available_variant.inventory_policy == 'continue' and product.selected_or_first_available_variant.requires_shipping and settings.enable_preorder_badge
      assign is_pre_order = true
    endif

    assign first_3d_model = product.media | where: 'media_type', 'model' | first

    assign custom_attributes = blank
    assign sibling_products = blank
    assign performance_bars = blank
  %}

  <div class="product-container page-width section-{{ section.id }}-padding">
    <div class="product-container-main product-media-layout--{{ media_layout }} product-media--{{ section.settings.media_size }} product-media--{{ section.settings.media_position }}{% if section.settings.enable_sticky_media %} product-scroll--sticky{% endif %}{% unless media_layout == 'stack' %}{% if section.settings.hide_thumbnail_mobile %} thumbnail--small-hide{% endif %}{% endunless %}">
      <div class="product-media-gallery">
        {% render 'product-media-gallery',
          product: product,
          variant_images: variant_images,
          media_layout: media_layout,
          enable_scale_image: true
        %}
      </div>
      <product-info
        class="product-info hidden-scrollbar"
        data-section="{{ section.id }}"
        data-url="{{ product.url }}"
        data-id="{{ product.id }}"
      >
        <div class="product-info-inner">
          {%- assign product_form_id = 'product-form-' | append: section.id -%}

          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when '@app' -%}
                <div class="product-info-app product-info-block">
                  {% render block %}
                </div>
              {%- when 'custom_liquid' -%}
                <div class="product-info-custom-liquid product-info-block" {{ block.shopify_attributes }}>
                  {{ block.settings.custom_liquid }}
                </div>
              {%- when 'text' -%}
                <div class="product-text product-info-block" {{ block.shopify_attributes }}>
                  {% assign description_id = 'Expandable-Container-'
                    | append: section.id
                    | append: '-'
                    | append: block.id
                  %}
                  {% render 'limit-height-description',
                    id: description_id,
                    content: block.settings.text,
                    if_limit_height: block.settings.limit_description_height
                  %}
                </div>
              {%- when 'title' -%}
                <div class="product-title product-info-block" {{ block.shopify_attributes }}>
                  {%- if block.settings.show_vendor and product.vendor != empty -%}
                    <span
                      class="product-vendor"
                      style="background: {{ block.settings.vendor_background }}; color: {{ block.settings.vendor_foreground }};"
                    >
                      {{ product.vendor | link_to_vendor: class: 'link' }}
                    </span>
                  {%- endif -%}
                  <h1 class="product-title-heading heading-size--{{ block.settings.size }}">{{ product.title }}</h1>

                  {%- if block.settings.show_rating
                    and product.metafields.reviews.rating.value != blank
                    or block.settings.enable_rating_preview
                  -%}
                    {% liquid
                      assign rating_value = product.metafields.reviews.rating.value.rating
                      assign rating_max_value = product.metafields.reviews.rating.value.scale_max
                      assign rating_total_count = product.metafields.reviews.rating_count

                      if block.settings.enable_rating_preview
                        assign rating_value = 4.5
                        assign rating_max_value = 5
                        assign rating_total_count = 1201
                      endif
                    %}
                    <a
                      class="product-rating link"
                      {% if block.settings.rating_target != blank %}
                        href="{{ block.settings.rating_target }}"
                      {% else %}
                        role="link"
                        aria-disabled="true"
                      {% endif %}
                    >
                      {%- render 'rating',
                        rating_value: rating_value,
                        scale_max: rating_max_value,
                        total_count: rating_total_count,
                        color: block.settings.star_color,
                        size: block.settings.star_size,
                        show_score: true,
                        show_total: block.settings.show_total_count
                      -%}
                    </a>
                  {%- endif -%}
                </div>
              {%- when 'price' -%}
                <div id="Price-{{ section.id }}" class="product-price-tax product-info-block" {{ block.shopify_attributes }}>
                  <div
                    class="product-price{% unless product.selected_or_first_available_variant.available %} hidden{% endunless %}"
                    role="status"
                  >
                    {%- render 'price',
                      product_object: product,
                      use_variant: true,
                      show_badges: block.settings.show_sale_badge,
                      badge_type: block.settings.sale_badge_type,
                      show_compare_at_price: true
                    -%}
                    {%- if is_pre_order -%}
                      <input type="hidden" name="properties[Sale type]" value="Pre-order" form="{{ product_form_id }}">
                    {%- endif -%}
                  </div>
                  {%- if block.settings.show_tax -%}
                    <div class="product-tax rte{% unless product.selected_or_first_available_variant.available %} hidden{% endunless %}">
                      {%- if cart.duties_included and cart.taxes_included -%}
                        {{ 'products.product.include_duties_and_taxes' | t }}
                      {%- elsif cart.taxes_included -%}
                        {{ 'products.product.include_taxes' | t }}
                      {%- elsif cart.duties_included -%}
                        {{ 'products.product.include_duties' | t }}
                      {%- endif -%}

                      {%- if shop.shipping_policy.body != blank -%}
                        {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                      {%- endif -%}
                    </div>
                  {%- endif -%}

                  {% comment %} 分期付款Banner {% endcomment %}
                  <div class="product-form{% unless product.selected_or_first_available_variant.available %} hidden{% endunless %}">
                    {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
                    {%- form 'product', product, id: product_form_installment_id, class: 'installment' -%}
                      <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                      {{ form | payment_terms }}
                    {%- endform -%}
                  </div>
                </div>
              {%- when 'inventory' -%}
                <div
                  id="Inventory-{{ section.id }}"
                  class="product-inventory product-info-block"
                  {{ block.shopify_attributes }}
                >
                  {%- if product.selected_or_first_available_variant.inventory_management == 'shopify'
                    and is_pre_order == false
                  -%}
                    {% render 'inventory-status',
                      variant: product.selected_or_first_available_variant,
                      threshold: block.settings.inventory_threshold,
                      low_alert_message: block.settings.low_stock_alert,
                      full_alert_message: block.settings.full_stock_alert,
                      show_bar: block.settings.show_percentage_bar,
                      low_bar_color: block.settings.low_stock_bar_color,
                      full_bar_color: block.settings.full_stock_bar_color
                    %}
                  {%- endif -%}
                </div>
              {%- when 'description' -%}
                {%- if product.description != blank -%}
                  <div
                    class="product-description product-info-block{% if block.settings.enable_brief %} light{% endif %}"
                    {{ block.shopify_attributes }}
                  >
                    {% liquid
                      if block.settings.enable_brief
                        assign description = product.description | strip_html | truncatewords: block.settings.limit
                      else
                        assign description = product.description
                      endif

                      assign description_id = 'Expandable-Container-' | append: section.id | append: '-' | append: block.id
                    %}
                    {% render 'limit-height-description',
                      id: description_id,
                      content: description,
                      if_limit_height: block.settings.limit_description_height
                    %}
                  </div>
                {%- endif -%}
              {%- when 'sku' -%}
                <div
                  id="Sku-{{ section.id }}"
                  class="product-sku product-info-block light"
                  role="status"
                  {{ block.shopify_attributes }}
                >
                  {%- if product.selected_or_first_available_variant.sku != blank -%}
                    <span>{{ 'products.product.sku' | t }}:</span>
                    {{ product.selected_or_first_available_variant.sku -}}
                  {%- endif -%}
                </div>
              {%- when 'collapsible_tab' -%}
                <div class="product-accordion product-info-block" {{ block.shopify_attributes }}>
                  <details-disclosure>
                    <details
                      class="details-disclosure"
                      {% if block.settings.default_expand %}
                        open
                      {% endif %}
                    >
                      <summary>
                        <h5 class="mb-0">
                          {{ block.settings.heading | default: block.settings.page.title }}
                        </h5>
                        {% render 'icon-sets', icon: 'caret' %}
                      </summary>
                      <div class="details-disclosure-content">
                        {%- if block.settings.show_product_description -%}
                          {{ product.description }}
                        {%- elsif block.settings.page.content != blank -%}
                          {{ block.settings.page.content }}
                        {%- elsif block.settings.content != blank -%}
                          {{ block.settings.content }}
                        {%- endif -%}
                        {%- if block.settings.show_in_box_products -%}
                          {% assign in_box_products = section.blocks | where: 'type', 'in_box_product' %}

                          {%- if in_box_products != empty -%}
                            <ul class="in-box-product-list list-unstyled">
                              {%- for in_box_product in in_box_products -%}
                                <li class="in-box-product-item hover-trigger" {{ in_box_product.shopify_attributes }}>
                                  <div class="media media--square">
                                    {%- if in_box_product.settings.image != blank -%}
                                      {{-
                                        in_box_product.settings.image
                                        | image_url: width: 200
                                        | image_tag: loading: 'lazy', class: 'animate--scale-up'
                                      -}}
                                    {%- else -%}
                                      {{ 'product-1' | placeholder_svg_tag: 'placeholder-svg' }}
                                    {%- endif -%}
                                  </div>
                                  <h3 class="h5 mb-0 font-body font-size-s">
                                    {{ in_box_product.settings.title | escape }}
                                  </h3>
                                </li>
                              {%- endfor -%}
                            </ul>
                          {%- endif -%}
                        {%- endif -%}
                      </div>
                    </details>
                  </details-disclosure>
                </div>
              {%- when 'popup' -%}
                {% if block.settings.bind_size_picker %}{% continue %}{% endif %}
                <div class="product-popup product-info-block" {{ block.shopify_attributes }}>
                  <modal-opener
                    role="button"
                    aria-haspopup="dialog"
                    aria-expanded="false"
                    aria-controls="PopupModal-{{ section.id }}-{{ block.id }}"
                  >
                    <a
                      href="{% if block.settings.page != blank %}{{ block.settings.page.url }}{% else %}#{% endif %}"
                      class="button button--small button--tertiary button--pill"
                    >
                      {%- if block.settings.icon != 'none' -%}
                        {% render 'icon-sets', icon: block.settings.icon %}
                      {%- endif -%}
                      {{ block.settings.text | escape }}
                    </a>
                  </modal-opener>
                </div>
              {%- when 'variant_picker' -%}
                {% comment %} 属性选择器 {% endcomment %}
                {% render 'product-variant-picker', product: product, block: block, product_form_id: product_form_id %}
              {%- when 'buy_buttons' -%}
                <div class="product-buy-button product-info-block" {{ block.shopify_attributes }}>
                  {%- render 'buy-buttons',
                    product: product,
                    product_form_id: product_form_id,
                    show_quantity_selector: block.settings.show_quantity_selector,
                    is_quantity_selector_standalone: block.settings.is_selector_standalone,
                    show_gift_card_recipient: block.settings.show_gift_card_recipient,
                    show_dynamic_checkout: block.settings.show_dynamic_checkout,
                    show_mini_checkout: block.settings.show_mini_checkout,
                    mini_checkout_position: block.settings.mini_checkout_position,
                    show_notify_form: block.settings.show_notify_form,
                    is_pre_order: is_pre_order
                  -%}
                </div>
              {%- when 'image' -%}
                <div
                  class="product-image product-info-block image-height--{{ block.settings.image_height }} hover-trigger"
                  {{ block.shopify_attributes }}
                >
                  <a
                    {% if block.settings.link != blank %}
                      href="{{ block.settings.link }}"
                    {% else %}
                      role="link"
                      aria-disabled="true"
                    {% endif %}
                  >
                    {% render 'lazy-image',
                      image_object: block.settings.image,
                      class: 'animate--scale-up',
                      sizes: '(min-width: 750px) 30rem, 100vw',
                      widths: '450, 660, 900',
                      placeholder: 'lifestyle-1'
                    %}
                  </a>
                </div>
              {%- when 'countdown' -%}
                <div
                  class="product-countdown product-info-block color-{{ block.settings.color_scheme }} gradient"
                  {{ block.shopify_attributes }}
                >
                  {%- unless block.settings.heading == blank and block.settings.discount_code == blank -%}
                    <div>
                      {%- if block.settings.heading != blank -%}
                        <h2 class="countdown-title{% if block.settings.discount_code == blank %} mb-0{% endif %}">
                          {{ block.settings.heading | escape }}
                        </h2>
                      {%- endif -%}

                      {%- if block.settings.discount_code != blank -%}
                        {% render 'discount-code', code: block.settings.discount_code %}
                      {%- endif -%}
                    </div>
                  {%- endunless -%}
                  {% render 'countdown',
                    end_time: block.settings.end_time,
                    end_text: block.settings.end_text,
                    hide_when_end: block.settings.hide_when_end,
                    simple: true,
                    timer_background: block.settings.timer_background_color,
                    timer_foreground: block.settings.timer_foreground_color
                  %}
                </div>
              {%- when 'payment_security' -%}
                {%- if block.settings.show_trust_badge or block.settings.show_payment_icons -%}
                  <div
                    class="product-payment-security product-info-block{% if block.settings.show_box %} show-box color-{{ block.settings.color_scheme }} gradient{% endif %}"
                    {{ block.shopify_attributes }}
                  >
                    <h2 class="h4">{{ 'products.product.payment_security' | t }}</h2>
                    {%- if block.settings.show_trust_badge -%}
                      <a
                        class="trust-badge"
                        href="https://www.shopify.com/security/pci-compliant?utm_medium=shop&utm_source=secure"
                        title="{{ 'sections.footer.trust_badge' | t }}"
                        target="_blank"
                        rel="nofollow"
                      >
                        {%- render 'shopify-trust-badge-svg' -%}
                      </a>
                    {%- endif -%}
                    {%- if block.settings.show_payment_icons and shop.enabled_payment_types.size > 0 -%}
                      <ul class="payment-icons list-unstyled">
                        {%- for type in shop.enabled_payment_types -%}
                          <li>
                            {{ type | payment_type_svg_tag: class: 'icon' }}
                          </li>
                        {%- endfor -%}
                      </ul>
                    {%- endif -%}
                  </div>
                {%- endif -%}
              {%- when 'related_categories' -%}
                {%- if product.collections.size > 0 and product.collections != blank -%}
                  <div class="product-related-categories product-info-block" {{ block.shopify_attributes }}>
                    <h2 class="h4">{{ 'products.product.related_categories' | t }}</h2>
                    {% render 'product-related-categories', product: product, max_to_show: block.settings.max_to_show %}
                  </div>
                {%- endif -%}
              {%- when 'complementary' -%}
                <product-recommendations
                  id="Product-Recommendations-{{ section.id }}"
                  class="product-complementary product-info-block quick-add-remove no-js-hidden{% if block.settings.show_separator_line %} product-info-block--with-top-border{% endif %}"
                  data-url="{{ routes.product_recommendations_url }}?section_id={{ section.id }}&product_id={{ product.id }}&limit={{ block.settings.product_list_limit }}&intent=complementary"
                  role="complementary"
                  {{ block.shopify_attributes }}
                >
                  <h2 class="h4">{{ block.settings.block_heading | escape }}</h2>
                  <div class="products-container">
                    <div class="complementary-slide loading-placeholder">
                      <ul class="list-unstyled">
                        {%- for i in (1..block.settings.products_per_page) -%}
                          <li>
                            {% render 'card-placeholder', show_horizontal: true %}
                          </li>
                        {%- endfor -%}
                      </ul>
                    </div>
                  </div>
                  {%- if recommendations.performed and recommendations.products_count > 0 -%}
                    <slider-component
                      class="recommendation-main slider"
                      data-slide-desktop
                      data-slide-mobile
                      data-draggable
                      data-slide-smooth
                    >
                      {% assign slider_id = 'Slider-Complementary-Product-' | append: block.id %}
                      <div class="slider-container">
                        {%- assign number_of_slides = recommendations.products_count
                          | plus: 0.0
                          | divided_by: block.settings.products_per_page
                          | ceil
                        -%}
                        <ul
                          id="Slider-Complementary-Product-{{ block.id }}"
                          class="slider-wrapper"
                          {% if number_of_slides > 1 %}
                            aria-label="{{ 'general.slider.name' | t }}"
                          {% endif %}
                        >
                          {%- for i in (1..number_of_slides) -%}
                            <li
                              id="Slide-{{ block.id }}-{{ forloop.index }}"
                              class="complementary-slide slider-slide"
                              {% if number_of_slides > 1 %}
                                aria-label="{{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length }}"
                              {% endif %}
                            >
                              <ul class="list-unstyled">
                                {% comment %}theme-check-disable UndefinedObject{% endcomment %}
                                {%- for product in recommendations.products
                                  limit: block.settings.products_per_page
                                  offset: continue
                                -%}
                                  <li class="focus-inset" tabindex="0">
                                    {%- render 'card-product',
                                      card_product: product,
                                      image_width: '200px',
                                      image_width_mobile: '200px',
                                      show_quick_add: settings.card_enable_quick_add,
                                      show_horizontal: true
                                    -%}
                                  </li>
                                {%- endfor -%}
                                {% comment %}theme-check-enable UndefinedObject{% endcomment %}
                              </ul>
                            </li>
                          {%- endfor -%}
                        </ul>
                      </div>
                      {% render 'slider-controller', slider_id: slider_id, show_dots: true %}
                    </slider-component>
                  {%- endif -%}
                </product-recommendations>
              {%- when 'icon_list' -%}
                <div class="product-icon-list product-info-block" {{ block.shopify_attributes }}>
                  {% render 'icon-list',
                    icon1: block.settings.icon_1,
                    image1: block.settings.image_1,
                    icon2: block.settings.icon_2,
                    image2: block.settings.image_2,
                    icon3: block.settings.icon_3,
                    image3: block.settings.image_3,
                    icon4: block.settings.icon_4,
                    image4: block.settings.image_4,
                    icon5: block.settings.icon_5,
                    image5: block.settings.image_5
                  %}
                </div>
              {%- when 'icon_text' -%}
                <div class="product-icon-text product-info-block" {{ block.shopify_attributes }}>
                  {%- if block.settings.image != blank -%}
                    {{ block.settings.image | image_url: width: 80 | image_tag }}
                  {%- elsif block.settings.icon != 'none' -%}
                    {% render 'icon-sets', icon: block.settings.icon %}
                  {%- endif -%}
                  {{ block.settings.description }}
                </div>
              {%- when 'custom_attribute' -%}
                {% liquid
                  if custom_attributes != blank
                    continue
                  endif

                  assign custom_attributes = section.blocks | where: 'type', 'custom_attribute'
                %}
                <div class="product-custom-attributes product-info-block" {{ block.shopify_attributes }}>
                  <table class="table-collapse">
                    <tbody>
                      {%- for attribute in custom_attributes -%}
                        {%- if attribute.settings.value != blank -%}
                          <tr>
                            <td>
                              <b>{{ attribute.settings.title | escape }}</b>
                            </td>
                            <td class="light">{{ attribute.settings.value }}</td>
                          </tr>
                        {%- endif -%}
                      {%- endfor -%}
                    </tbody>
                  </table>
                </div>
              {%- when 'sibling_product' -%}
                {% liquid
                  if sibling_products != blank
                    continue
                  endif

                  assign sibling_products = section.blocks | where: 'type', 'sibling_product'
                %}
                <div
                  class="product-sibling-products product-info-block auto-grid quick-add-remove"
                  style="--item-min-size: 120px; --item-min-size-mobile: 80px;"
                >
                  {%- for sibling_product in sibling_products -%}
                    {% liquid
                      assign sibling_image = sibling_product.settings.image | default: sibling_product.settings.product.featured_image
                      assign sibling_title = sibling_product.settings.title | default: sibling_product.settings.product.title
                      assign sibling_index = forloop.index0 | modulo: 6 | plus: 1
                      assign sibling_placeholder = 'product-' | append: sibling_index
                    %}
                    <div
                      class="sibling-product grid-item hover-trigger{% if product == sibling_product.settings.product %} sibling-active{% endif %}"
                      {{ sibling_product.shopify_attributes }}
                    >
                      <div class="sibling-product-image media media--square sm-round">
                        {% render 'lazy-image',
                          image_object: sibling_image,
                          class: 'animate--scale-up',
                          widths: '165, 360, 450, 660',
                          sizes: '200px',
                          placeholder: sibling_placeholder
                        %}
                      </div>
                      {%- if sibling_title != blank -%}
                        <div class="sibling-product-title">
                          <a href="{{ sibling_product.settings.product.url }}" class="link link-full font-size-s">
                            {{- sibling_title | escape -}}
                          </a>
                        </div>
                      {%- endif -%}
                      {% render 'price',
                        product_object: sibling_product.settings.product,
                        class: 'sibling-product-price'
                      %}
                    </div>
                  {%- endfor -%}
                </div>
              {%- when 'separator' -%}
                <div
                  class="product-separator-line product-info-block quick-add-remove"
                  style="margin-top: {{ block.settings.margin_top }}px; margin-bottom: {{ block.settings.margin_bottom }}px;"
                  {{ block.shopify_attributes }}
                ></div>
              {%- when 'gift' -%}
                {% liquid
                  assign gift_list = product.selected_or_first_available_variant.metafields.custom.variant_gift.value
                  if gift_list == blank
                    assign gift_list = product.metafields.custom.product_gift.value
                  endif
                %}
                <div
                  id="Product-Gift-{{ section.id }}"
                  class="product-gift product-info-block{% if block.settings.show_separator_line %} product-info-block--with-top-border{% endif %}"
                  role="region"
                  {{ block.shopify_attributes }}
                >
                  {%- unless gift_list == blank -%}
                    <h2 class="h4">{{ block.settings.block_heading | escape }}</h2>
                    <ul class="gift-list-wrapper">
                      {%- for gift in gift_list limit: 6 -%}
                        <li>
                          <input
                            type="hidden"
                            name="properties[gift-{{ forloop.index }}]"
                            value="{{ gift.title | escape }}"
                            form="{{ product_form_id }}"
                          >
                          <div class="gift-item hover-trigger">
                            <div class="gift-media media media--square">
                              {{-
                                gift.image
                                | image_url: width: 200
                                | image_tag: loading: 'lazy', class: 'animate--scale-up'
                              -}}
                            </div>
                            <div class="gift-information">
                              <div class="gift-badge">
                                <span class="badge product-gift-badge">{{ 'products.product.has_free' | t }}</span>
                              </div>
                              <h3 class="gift-title font-body">{{- gift.title | escape -}}</h3>
                              <small
                                ><s>{{ gift.price.value | money_with_currency }}</s></small
                              >
                            </div>
                          </div>
                        </li>
                      {%- endfor -%}
                    </ul>
                  {%- endunless -%}
                </div>
              {%- when 'pickup-availability' -%}
                {%- assign pick_up_availabilities = product.selected_or_first_available_variant.store_availabilities
                  | where: 'pick_up_enabled', true
                -%}
                <pickup-availability
                  class="product-pickup-availabilities product-info-block no-js-hidden quick-add-remove"
                  {% if product.selected_or_first_available_variant.available and pick_up_availabilities.size > 0 %}
                    available
                  {% endif %}
                  data-root-url="{{ routes.root_url }}"
                  data-variant-id="{{ product.selected_or_first_available_variant.id }}"
                  {{ block.shopify_attributes }}
                ></pickup-availability>
                <script src="{{ 'pickup-availability.js' | asset_url }}" defer="defer"></script>
              {%- when 'performance_bar' -%}
                {% liquid
                  if performance_bars != blank
                    continue
                  endif

                  assign performance_bars = section.blocks | where: 'type', 'performance_bar'
                %}
                <div class="product-performance-bars product-info-block">
                  {%- for performance_bar in performance_bars -%}
                    {% liquid
                      if performance_bar.settings.value < 4
                        assign bar_color = performance_bar.settings.color_low
                      elsif performance_bar.settings.value < 8
                        assign bar_color = performance_bar.settings.color_medium
                      else
                        assign bar_color = performance_bar.settings.color_high
                      endif
                    %}
                    <div class="product-performance-bar" {{ performance_bar.shopify_attributes }}>
                      <div class="performance-labels">
                        <small class="light">{{ performance_bar.settings.label_low | escape }}</small>
                        <small class="light">{{ performance_bar.settings.label_medium | escape }}</small>
                        <small class="light">{{ performance_bar.settings.label_high | escape }}</small>
                      </div>
                      <div
                        class="performance-bar"
                        style="--bar-color: {{ bar_color.red }}, {{ bar_color.green }}, {{ bar_color.blue }}; "
                      >
                        {%- for i in (1..10) -%}
                          <span class="bar-item{% if i <= performance_bar.settings.value %} active{% endif %}"></span>
                        {%- endfor -%}
                      </div>
                      {%- if performance_bar.settings.heading != blank -%}
                        <div class="performance-name">{{ performance_bar.settings.heading | escape }}</div>
                      {%- endif -%}
                      {%- if performance_bar.settings.description != blank -%}
                        <div class="performance-description light">
                          {{ performance_bar.settings.description }}
                        </div>
                      {%- endif -%}
                    </div>
                  {%- endfor -%}
                </div>
              {%- when 'delivery_eta' -%}
                {% liquid
                  assign current_timestamp = 'now' | date: '%s' | plus: 0

                  assign delta_before_timestamp = block.settings.processing_time | minus: block.settings.time_variance | times: 86400
                  assign before_date_timestamp = current_timestamp | plus: delta_before_timestamp
                  assign before_date = before_date_timestamp | date: format: 'month_day'

                  assign delta_after_timestamp = block.settings.processing_time | plus: block.settings.time_variance | times: 86400
                  assign after_date_timestamp = current_timestamp | plus: delta_after_timestamp
                  assign after_date = after_date_timestamp | date: format: 'month_day'
                %}
                <div
                  class="product-delivery-eta color-{{ block.settings.color_scheme }} gradient product-info-block"
                  {{ block.shopify_attributes }}
                >
                  {% render 'icon-sets', icon: 'clock' %}
                  <b>{{ 'sections.cart.estimate_delivery' | t }}: {{ before_date }} - {{ after_date }}</b>
                </div>
            {%- endcase -%}
          {%- endfor -%}

          <div class="product-info-others">
            {%- if section.settings.enable_share -%}
              {%- liquid
                assign share_url = product.selected_variant.url | default: product.url | prepend: request.origin
                assign title = product.title
                assign media = product.featured_image
                if product.selected_variant != blank
                  assign title = title | append: ',' | append: product.selected_variant.title
                  assign media = product.selected_variant.featured_image
                endif
              -%}
              <div id="Share-Link-{{ section.id }}" class="product-share-link">
                {% render 'share-links',
                  link: share_url,
                  title: title,
                  media: media,
                  style: 'wrapper',
                  show_tooltip: true
                %}
              </div>
            {%- endif -%}

            {%- if settings.enable_breadcrumb -%}
              <div class="product-breadcrumb quick-add-remove">
                {%- render 'breadcrumb', page_type: 'product' -%}
              </div>
            {%- endif -%}

            <a
              href="{{ product.url }}"
              class="product-view-details button button--small button--tertiary button--pill button--hover-animate focus-inset"
            >
              {% render 'icon-sets', icon: 'eye' %}
              {{ 'general.view_details' | t }}
            </a>
          </div>
        </div>
      </product-info>
    </div>
    <div class="product-container-bottom quick-add-remove">
      {% assign custom_tabs = section.blocks | where: 'type', 'custom_tab' %}
      {%- unless custom_tabs == empty and section.settings.show_description_tab == false -%}
        <tab-panel class="product-tab-panel tab-panel">
          <scrollable-content-viewer class="scrollable-content" data-draggable>
            <div class="tab-panel-tabs scrollable-content-wrapper" role="tablist">
              {%- if section.settings.show_description_tab -%}
                <div
                  id="Product-Tab-Description"
                  class="tab focus-inset"
                  role="tab"
                  tabindex="0"
                  aria-controls="Product-Panel-Description"
                  aria-selected="true"
                >
                  {%- if section.settings.show_description_icon -%}
                    {% render 'icon-sets', icon: 'description' %}
                  {%- endif -%}
                  {{ 'products.product.description' | t }}
                </div>
              {%- endif -%}
              {%- for block in custom_tabs -%}
                <div
                  id="Product-Tab-{{ block.id }}"
                  class="tab focus-inset"
                  role="tab"
                  tabindex="0"
                  aria-controls="Product-Panel-{{ block.id }}"
                  {% if section.settings.show_description_tab == false and forloop.first %}
                    aria-selected="true"
                  {% endif %}
                  {{ block.shopify_attributes }}
                >
                  {%- if block.settings.image != blank -%}
                    {{ block.settings.image | image_url: width: 50 | image_tag }}
                  {%- elsif block.settings.icon != 'none' -%}
                    {% render 'icon-sets', icon: block.settings.icon %}
                  {%- endif -%}
                  {{ block.settings.tab_title | escape }}
                </div>
              {%- endfor -%}
            </div>
          </scrollable-content-viewer>
          <div class="tab-panel-panels">
            {%- if section.settings.show_description_tab -%}
              <div
                id="Product-Panel-Description"
                class="panel rte{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}"
                role="tabpanel"
                aria-labelledby="Product-Tab-Description"
              >
                {{ product.description }}
              </div>
            {%- endif -%}
            {%- for block in custom_tabs -%}
              <div
                id="Product-Panel-{{ block.id }}"
                class="panel{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}"
                role="tabpanel"
                {% unless section.settings.show_description_tab == false and forloop.first %}
                  aria-hidden="true"
                {% endunless %}
                aria-labelledby="Product-Tab-{{ block.id }}"
              >
                {%- if block.settings.html != blank -%}
                  {{ block.settings.html }}
                {%- else -%}
                  <div class="rte">
                    {%- if block.settings.page != blank -%}
                      {{ block.settings.page.content }}
                    {%- elsif block.settings.show_description -%}
                      {{ product.description }}
                    {%- else -%}
                      {{ block.settings.content }}
                    {%- endif -%}
                  </div>
                {%- endif -%}
              </div>
            {%- endfor -%}
          </div>
        </tab-panel>
      {%- endunless -%}

      {% assign shopify_metafields_size = product.metafields.shopify | size %}
      {%- if section.settings.show_category_metafields and shopify_metafields_size > 0 -%}
        <div class="specification-table">
          <table class="table-collapse">
            <caption class="visually-hidden">
              {{ 'products.product.specification.table' | t }}
            </caption>
            <thead class="visually-hidden">
              <tr>
                <th>{{ 'products.product.specification.name' | t }}</th>
                <th>{{ 'products.product.specification.value' | t }}</th>
              </tr>
            </thead>
            <tbody>
              {%- for metafield in product.metafields.shopify -%}
                {%- if metafield[1].value.count > 0 -%}
                  {% assign translation_key = 'metafields.shopify.' | append: metafield[0] %}
                  <tr>
                    <td>
                      <b>{{- translation_key | t -}}</b>
                    </td>
                    <td>
                      {%- for value in metafield[1].value -%}
                        {{- value.label -}}
                        {% unless forloop.last %}, {% endunless %}
                      {%- endfor -%}
                    </td>
                  </tr>
                {%- endif -%}
              {%- endfor -%}
            </tbody>
          </table>
        </div>
      {%- endif -%}
    </div>
  </div>

  {% comment %} 一般Pop 弹窗 {% endcomment %}
  {% assign popups = section.blocks | where: 'type', 'popup' %}
  {%- for block in popups -%}
    <modal-dialog
      id="PopupModal-{{ section.id }}-{{ block.id }}"
      class="product-popup-modal modal"
      {{ block.shopify_attributes }}
    >
      <div class="modal-overlay"></div>
      <div
        class="modal-inner gradient focus-none"
        role="dialog"
        data-trap
        aria-modal="true"
        tabindex="-1"
      >
        <button
          type="button"
          class="modal-close button button--icon button--ethereal focus-inset"
          aria-label="{{ 'accessibility.close' | t }}"
          onclick="this.closest('modal-dialog').hide()"
        >
          {% render 'icon-sets', icon: 'close' %}
        </button>
        <div class="modal-main modal-content">
          {%- if block.settings.page != blank -%}
            <h1 class="h2">{{ block.settings.page.title }}</h1>
          {%- endif -%}
          <div class="rte">
            {%- if block.settings.page != blank -%}
              {{ block.settings.page.content }}
            {%- else -%}
              {%- if block.settings.content != blank -%}
                {{ block.settings.content }}
              {%- endif -%}
              {%- if block.settings.image != blank -%}
                {{ block.settings.image | image_url: width: 800 | image_tag: loading: 'lazy' }}
              {%- endif -%}
            {%- endif -%}
          </div>
        </div>
      </div>
    </modal-dialog>
  {%- endfor -%}

  {% assign buy_button = section.blocks | where: 'type', 'buy_buttons' | first %}
  {% comment %} Notify Form {% endcomment %}
  {%- if buy_button != blank and buy_button.settings.show_notify_form -%}
    {% render 'product-notify-form-modal' %}
  {%- endif -%}

  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>

  {%- if product.media.size > 0 -%}
    <script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {% comment %} 3D相关 S {% endcomment %}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <script type="application/json" id="ProductJSON-{{ product.id }}">
      {{ product.media | where: 'media_type', 'model' | json }}
    </script>
    <script src="{{ 'product-model.js' | asset_url }}" defer></script>
  {%- endif -%}
  {% comment %} 3D相关 E {% endcomment %}
</div>

{%- if product.media.size > 0 -%}
  {% comment %} 产品相册放大 {% endcomment %}
  {% render 'product-media-modal', variant_images: variant_images %}
  <script src="{{ 'product-gallery-modal.js' | asset_url }}" defer="defer"></script>
{%- endif -%}
{% comment %} 数据结构 {% endcomment %}
<script type="application/ld+json">
  {{ product | structured_data }}
</script>

